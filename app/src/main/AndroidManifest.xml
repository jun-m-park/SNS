<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.junmp.teamproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application

        android:allowBackup="true"
        android:icon="@drawable/nupdoung"
        android:label="Nupdoong SNS"
        android:roundIcon="@drawable/nupdoung"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FeedActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity android:name=".SignupActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".UploadProfileIMGActivity"
            android:label="@string/title_activity_upload_profile_img"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".WritingPostActivity"
            android:theme="@style/AppTheme.NoActionBar"/>

        <service
            android:name=".MusicService"
            android:enabled="true"
            android:exported="true"></service>
    </application>

</manifest>